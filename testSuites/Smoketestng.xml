<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
		
	<groups>
		<run>
			<include name="Smoke"></include>
		</run>
	</groups>
	
	<!-- Listeners section -->
  <listeners>
    <listener class-name="testautomationproject.TestComponents.Listeners"/>
  </listeners>
  <test thread-count="1" name="Submit order Test">
    <classes>
      <class name="testautomationproject.Seleniumprojectjava.StandaloneTest"/>
      
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="1" name="Error Validations">
    <classes>
      
      <class name="testautomationproject.Seleniumprojectjava.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->











